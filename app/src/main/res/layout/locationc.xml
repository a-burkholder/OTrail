<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#C65C16"
    android:backgroundTint="#C65C16">

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="1267dp"
        android:layout_height="617dp"
        android:background="#C65C16"
        android:backgroundTint="#C65C16"
        app:srcCompat="@drawable/img_8"
        tools:layout_editor_absoluteX="-198dp"
        tools:layout_editor_absoluteY="-133dp" />

    <TextView
        android:id="@+id/textView96"
        android:layout_width="249dp"
        android:layout_height="78dp"
        android:background="#C65C16"
        android:text="Welcome to the Big Blue River Crossing"
        android:textColor="#FFFFFF"
        android:textSize="28sp"
        tools:layout_editor_absoluteX="314dp"
        tools:layout_editor_absoluteY="30dp" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="597dp"
        android:layout_height="272dp"
        app:srcCompat="@drawable/img_4"
        tools:layout_editor_absoluteX="-130dp"
        tools:layout_editor_absoluteY="106dp" />

    <TextView
        android:id="@+id/textView97"
        android:layout_width="184dp"
        android:layout_height="214dp"
        android:text="The Blue River is about 359 miles long. It goes from central Nebraska into Kansas, until its confluence with the Kansas River at Manhattan."
        android:textColor="#000000"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="80dp"
        tools:layout_editor_absoluteY="133dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="200dp"
        android:layout_height="58dp"
        android:backgroundTint="#c65C16"
        android:text="Talk To A Local"
        tools:layout_editor_absoluteX="643dp"
        tools:layout_editor_absoluteY="347dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#C65C16"
        android:text="Continue On The Trail"
        tools:layout_editor_absoluteX="632dp"
        tools:layout_editor_absoluteY="280dp" />
</androidx.constraintlayout.widget.ConstraintLayout>
