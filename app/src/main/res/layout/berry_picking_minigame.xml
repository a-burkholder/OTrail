<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="928dp"
        android:layout_height="570dp"
        android:elevation="0dp"
        android:scaleX="0.95"
        android:scaleY="0.95"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/berrybackground" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="55dp"
        android:layout_height="60dp"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="324dp"
        android:background="#004000"
        android:elevation="1dp"
        app:layout_constraintEnd_toStartOf="@+id/berryscore"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:srcCompat="@drawable/berry" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="43dp"
        android:layout_height="45dp"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="76dp"
        android:background="#004000"
        android:elevation="1dp"
        android:scaleX="1.2"
        android:scaleY="1.2"
        app:layout_constraintEnd_toStartOf="@+id/berryscore"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:srcCompat="@drawable/berry" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="43dp"
        android:layout_height="45dp"
        android:layout_marginTop="188dp"
        android:layout_marginEnd="88dp"
        android:background="#004000"
        android:elevation="1dp"
        android:scaleX="1.2"
        android:scaleY="1.2"
        app:layout_constraintEnd_toStartOf="@+id/berryscore"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:srcCompat="@drawable/berry" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="43dp"
        android:layout_height="45dp"
        android:layout_marginTop="212dp"
        android:layout_marginEnd="396dp"
        android:background="#004000"
        android:elevation="1dp"
        android:scaleX="1.2"
        android:scaleY="1.2"
        app:layout_constraintEnd_toStartOf="@+id/berryscore"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:srcCompat="@drawable/berry" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="43dp"
        android:layout_height="45dp"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="220dp"
        android:background="#004000"
        android:elevation="1dp"
        android:scaleX="1.2"
        android:scaleY="1.2"
        app:layout_constraintEnd_toStartOf="@+id/berryscore"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:srcCompat="@drawable/berry" />

    <TextView
        android:id="@+id/berryscore"
        android:layout_width="60dp"
        android:layout_height="33dp"
        android:layout_marginStart="132dp"
        android:layout_marginBottom="124dp"
        android:elevation="10dp"
        android:text="0"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintBottom_toTopOf="@+id/centerTimer"
        app:layout_constraintStart_toEndOf="@+id/centerTimer" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="40dp"
        android:layout_height="37dp"
        android:layout_marginEnd="204dp"
        android:layout_marginBottom="120dp"
        android:elevation="10dp"
        android:text="8"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintBottom_toTopOf="@+id/centerTimer"
        app:layout_constraintEnd_toEndOf="@+id/centerTimer" />

    <TextView
        android:id="@+id/centerTimer"
        android:layout_width="279dp"
        android:layout_height="60dp"
        android:elevation="40dp"
        android:text="Center Box"
        android:textAlignment="center"
        android:textColor="#D4EEC4"
        android:textSize="34sp"
        android:typeface="serif"
        android:visibility="visible"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.446"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.529" />

    <Button
        android:id="@+id/returnButton"
        android:layout_width="163dp"
        android:layout_height="85dp"
        android:backgroundTint="#004000"
        android:text="Continue"
        android:textColor="#D5EFC5"
        android:textSize="20dp"
        android:typeface="serif"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.975"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.938" />

</androidx.constraintlayout.widget.ConstraintLayout>