<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#C65C16"
    android:backgroundTint="#C65C16">

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="961dp"
        android:layout_height="633dp"
        android:background="#C65C16"
        android:backgroundTint="#C65C16"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/img_8" />

    <TextView
        android:id="@+id/textView96"
        android:layout_width="249dp"
        android:layout_height="78dp"
        android:layout_marginStart="328dp"
        android:layout_marginTop="16dp"
        android:background="#C65C16"
        android:text="Welcome to the Big Blue River Crossing"
        android:textColor="#FFFFFF"
        android:textSize="28sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="331dp"
        android:layout_height="292dp"
        android:layout_marginTop="92dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/img_4" />

    <TextView
        android:id="@+id/textView97"
        android:layout_width="200dp"
        android:layout_height="235dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="120dp"
        android:text="The Blue River is about 359 miles long. It goes from central Nebraska into Kansas, until its confluence with the Kansas River at Manhattan."
        android:textColor="#000000"
        android:textSize="22sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="468dp"
        android:layout_marginTop="316dp"
        android:backgroundTint="#C65C16"
        android:text="Continue On The Trail"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="240dp"
        android:layout_marginBottom="32dp"
        android:text="[2]"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/imageView17"
        app:layout_constraintEnd_toStartOf="@+id/textView96" />
</androidx.constraintlayout.widget.ConstraintLayout>
